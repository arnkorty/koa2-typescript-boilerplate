{
  "name": "koa2-typescript-boilerplate",
  "version": "1.0.0",
  "engines": {
    "node": "6.9.1"
  },
  "description": "A boilerplate for using Koa2 with TypeScript",
  "main": "build/index.js",
  "scripts": {
    "test": "tsc && mocha build/test",
    "build": "tsc && ",
    "start": "node build/index.js",
    "dev-server": "nodemon -w lib -x ts-node lib/index.ts",
    "lint": "tslint lib/**/*.ts",
    "migrate": "tsc && ./node_modules/sequelize-cli/bin/sequelize db:migrate",
    "migrate:undo": "tsc && ./node_modules/sequelize-cli/bin/sequelize db:migrate:undo",
    "seed": "tsc && ./node_modules/sequelize-cli/bin/sequelize db:seed",
    "seed:undo": "tsc && ./node_modules/sequelize-cli/bin/sequelize db:seed:undo"
  },
  "keywords": [
    "typescript",
    "koa",
    "koa2"
  ],
  "author": "Ã†ndrew Rininsland <aendrew@aendrew.com>",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/chalk": "^0.4.31",
    "@types/form-data": "0.0.32",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/koa": "^2.0.34",
    "@types/koa-bodyparser": "^3.0.20",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-router": "^7.0.21",
    "@types/lodash": "^4.14.64",
    "@types/log4js": "0.0.32",
    "@types/node": "^6.0.73",
    "@types/qs": "^6.2.31",
    "@types/sequelize": "^4.0.54",
    "bcrypt": "^1.0.2",
    "chalk": "^1.1.3",
    "dotenv": "^2.0.0",
    "jsonwebtoken": "^7.4.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-router": "^7.0.1",
    "log4js": "^1.1.1",
    "mysql": "^2.13.0",
    "mysql2": "^1.2.0",
    "passthrough-counter": "^1.0.0",
    "qs": "^6.4.0",
    "reify": "^0.11.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
