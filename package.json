    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0",
    "sequelize": "^3.30.4",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.2.0",
    "ts-node": "^3.0.4",
    "tslint": "^3.15.1",
    "typescript": "^2.3.2"
  },
  "repository": {
    "url": "git@github.com:aendrew/koa2-typescript-boilerplate.git",
    "type": "git"
  }
